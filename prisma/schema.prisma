// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            Int         @default(autoincrement()) @id
  phone         String      @unique
  name          String?
  last          String?
  age           Int?
  gender        String?
  weight        Int?
  height        Int?
  token         String?     @unique
}
